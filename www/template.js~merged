  return ((stack1 = container.invokePartial(partials.nav,depth0,{"name":"nav","data":data,"helpers":helpers,"partials":partials,"decorators":container.decorators})) != null ? stack1 : "")
    + "\r\n<!-- ===============================================-->\r\n<!--    Main Content-->\r\n<!-- ===============================================-->\r\n\r\n<main>\r\n\r\n\r\n    <!-- ============================================-->\r\n    <!-- Preloader ==================================-->\r\n    <div class=\"preloader\" id=\"preloader\">\r\n        <div class=\"preloader-wrapper big active\">\r\n            <div class=\"spinner-layer spinner-white-only\">\r\n                <div class=\"circle-clipper left\">\r\n                    <div class=\"circle\"></div>\r\n                </div>\r\n                <div class=\"gap-patch\">\r\n                    <div class=\"circle\"></div>\r\n                </div>\r\n                <div class=\"circle-clipper right\">\r\n                    <div class=\"circle\"></div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <!-- ============================================-->\r\n    <!-- End of Preloader ===========================-->\r\n    <section class=\"py-0 overflow-hidden\" id=\"top\">\r\n        <div class=\"flexslider border-0 overflow-hidden rounded-0\">\r\n            <ul class=\"slides\">\r\n                <li data-zanim-timeline='{\"delay\":0.1}'>\r\n                    <div class=\"bg-holder overlay_header parallax\"\r\n                         style=\"background-image:url(../assets/img/technology/strategies_header.jpg);\">\r\n                    </div>\r\n                    <!--/.bg-holder-->\r\n\r\n                    <div class=\"header-overlay\"></div>\r\n                    <div class=\"container\">\r\n                        <div class=\"d-flex align-items-center pt-10 pb-8\">\r\n                            <div class=\"header-text\">\r\n                                <div class=\"overflow-hidden\">\r\n                                    <h1 class=\"display-3 text-white fs-5 fs-md-7\"\r\n                                        data-zanim-xs='{\"duration\":2,\"delay