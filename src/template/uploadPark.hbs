{{> navCreate}}
<!-- ===============================================-->
<!--    Main Content-->
<!-- ===============================================-->
<main>


    <!-- ============================================-->
    <!-- Preloader ==================================-->
    <div class="preloader" id="preloader">
        <div class="preloader-wrapper big active">
            <div class="spinner-layer spinner-white-only">
                <div class="circle-clipper left">
                    <div class="circle"></div>
                </div>
                <div class="gap-patch">
                    <div class="circle"></div>
                </div>
                <div class="circle-clipper right">
                    <div class="circle"></div>
                </div>
            </div>
        </div>
    </div>
    <!-- ============================================-->
    <!-- End of Preloader ===========================-->

    <!-- ============================================-->
    <!-- <section> begin ============================-->
    <section class="text-center pb-0" style="">

        <div class="container-fluid">


            <div class="overflow-hidden align-content-center" align="center">
                <div class="col-md-8" align="center">
                    <h3 class="fs-sm-5 mb-2" data-zanim-xs='{"duration":"1.5","delay":0}'>Upload Your Parks
                    </h3>
                </div>
            </div>
            <br>
            <div class="overflow-hidden">
                <hr class="hr-short border-black" data-zanim-xs='{"duration":"1.5","delay":0.2}'/>
            </div>
        </div>
        <!-- end of .container-->


        <div class="container" align="left">
            <form>
                <div class="form-group">
                    <label for="title">Park Name</label>
                    <input class="form-control" id="park-name" type="text" placeholder="Park Name" maxlength="100">
                </div>
                <br>
                <div class="form-group">
                    <label for="caption_1">Park Address</label>
                    <input class="form-control" id="park-address" type="text"
                           placeholder="12345 Street, City, State 12345" maxlength="100">
                </div>
                <br>
                <div class="form-group">
                    <label for="caption_2">Park Bio</label>
                    <input class="form-control" id="park-bio" type="text" placeholder="Park Bio" maxlength="1000">
                </div>
                <br>
                <label class="" for="">Features</label>
                <div class="form-group form-check">
                    <div class="row">
                        <div class="col-2">
                            <input class="form-check-input" id="defaultcheckbox1" type="checkbox">
                            <label class="form-check-label" for="defaultcheckbox1">Pavilions</label>
                        </div>
                        <div class="col-2">
                            <input class="form-check-input" id="defaultcheckbox2" type="checkbox">
                            <label class="form-check-label" for="defaultcheckbox2">Tennis Courts</label>
                        </div>
                        <div class="col-2">
                            <input class="form-check-input" id="defaultcheckbox3" type="checkbox">
                            <label class="form-check-label" for="defaultcheckbox3">Baseball Fields</label>
                        </div>
                        <div class="col-2">
                            <input class="form-check-input" id="defaultcheckbox4" type="checkbox">
                            <label class="form-check-label" for="defaultcheckbox4">Soccer Fields</label>
                        </div>
                        <div class="col-2">
                            <input class="form-check-input" id="defaultcheckbox5" type="checkbox">
                            <label class="form-check-label" for="defaultcheckbox5">Basketball Courts</label>
                        </div>
                    </div>
                </div>
                <br>
                <div class="form-group">
                    <label for="exampleFormControlFile1">Park Picture</label>
                    <input class="form-control-file" id="exampleFormControlFile1" type="file">
                </div>
                <br>
                <div class="custom-control custom-switch">
                    <input class="custom-control-input" id="active" type="checkbox" style="background-color: #D8F2D1 !important" checked>
                    <label class="custom-control-label" for="active">Active</label>
                </div>
                <br>


            </form>
            <button class="btn btn-primary mb-3 saveAGGRCCreate" type="submit">Save</button>
        </div>


    </section>
    <!-- <section> close ============================-->
    <!-- ============================================-->


</main>
<!-- ===============================================-->
<!--    End of Main Content-->
<!-- ===============================================-->


{{>footer}}
