<div class="container">
    <div class="col-12">
        <img class="img-fluid" src="../assets/img/create/header_blank.jpg">
    </div>
</div>
<div class="container">
    <h2>Headers</h2>
<form>
    <div class="form-group">
        <label for="title">Title</label>
        <input class="form-control" id="title" type="text" placeholder="Title">
    </div>
    <div class="form-group">
        <label for="caption_1">1st Line of Caption</label>
        <input class="form-control" id="caption_1" type="text" placeholder="1st Line of Caption">
    </div>
    <div class="form-group">
        <label for="caption_2">2nd Line of Caption (this is only on two lines on small screens)</label>
        <input class="form-control" id="caption_2" type="text" placeholder="2nd Line of Caption">
    </div>
    <div class="form-group">
        <label for="button_text">Button Text</label>
        <input class="form-control" id="button_text" type="text" placeholder="Button Text">
    </div>
    <div class="form-group">
        <label for="button_link">Button Link</label>
        <input class="form-control" id="button_link" type="text" placeholder="Button Link">
    </div>
    <div class="form-group">
    <select class="custom-select mb-3">
        <label for="slide_number">Slide Number</label>
        <option selected="">Slide number</option>
        <option value="1">1</option>
        <option value="2">2</option>
        <option value="3">3</option>
        <option value="4">4</option>
        <option value="5">5</option>
        <option value="6">6</option>
        <option value="7">7</option>
        <option value="8">8</option>
        <option value="9">9</option>
    </select>
    </div>
    <div class="custom-file">
        <label class="custom-file-label" for="picture">Header Picture</label>
        <input class="custom-file-input"  id="picture" type="file">
    </div>
    <div class="custom-control custom-switch">
        <input class="custom-control-input" id="active" type="checkbox" checked>
        <label class="custom-control-label" for="active">Active</label>
    </div>


</form>
<button class="btn btn-primary mb-3 saveAGGRCCreate" type="submit">Save</button>
</div>
